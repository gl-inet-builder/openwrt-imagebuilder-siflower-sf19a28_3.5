
config service 'general'
	option port '83'

config service 'ddns'
	option enabled '0'

config service 'download'

config service 'adblock'
	option enable '0'

config service 'autoupdate'
	option time '04:00'
	option enable '0'

config service 'samba'
	option read_only 'yes'

config service 'openvpn'
	option enable '0'
	option force '0'

config service 'repeater'
	option autoconnect '1'
